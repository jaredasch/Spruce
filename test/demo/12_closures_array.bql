func makeCounter () -> function {
    let i : int = 0;
    let count : function = func () -> int {
        i = i + 1;
        return i;
    };
    return count;
}

let x : [function] = [makeCounter(), makeCounter()];
let x0 : function = x[0];
let x1 : function = x[1];
x0();
x1();
x0();
return x1();
